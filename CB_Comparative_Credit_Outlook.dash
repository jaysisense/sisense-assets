var pvalue = decodeURIComponent(s2[1]);\n\n\t\t\n\t\t\n\t\t\n\t\tif(pname == 'ec') {\n\t\t\tjaql.datasource = pvalue;\n\t\t}\n\t\telse if(pname == 'dim') {\n\t\t\tdimA = pvalue.split('|');\n\t\t}\n\t\telse if(pname == 'msr') {\n\t\t\tmsrA = pvalue.split('|');\n\t\t}\n\t\telse if(pname.substring(0,6) == 'filter') {\n\t\t\tvar f = {\n\t\t\t\tfname: pname,\n\t\t\t\tfvalue: pvalue\n\t\t\t}\n\t\t\t\n\t\t\tfilterA.push(f);\n\t\t}\t\t\n\t}\n\t\n\t\n\tfor(var i = 0; i < dimA.length; i++) {\n\t\tvar j = {\n\t\t\tdim: ''\n\t\t};\n\t\t\n\t\tj.dim = '[' + dimA[i] + ']';\t\n\t\tjaql.metadata.push(j);\n\t}\n\n\tfor(var i = 0; i < msrA.length; i++) {\n\t\tvar j = {\n\t\t\tdim: '',\n\t\t\tagg: 'sum'\n\t\t};\n\t\t\n\t\tj.dim = '[' + msrA[i] + ']';\t\n\t\tjaql.metadata.push(j);\n\t}\t\n\t\n\tfor(var i = 0; i < filterA.length; i++) {\n\t \n\t\tvar j = {\n\t\t\tpanel: 'scope'\n\t\t};\n\t\t\n\t\tvar fo = filterA[i];\n\t\t\n\t\tif(fo.fname.indexOf('filtermem') > -1) {\n\t\t\tj.dim = '[' + fo.fname.substring(10) + ']';\n\t\t\tj.members = fo.fvalue.split('|');\n\t\t}\n\t\telse if(fo.fname.indexOf('filterrange') > -1) {\n\t\t\tj.dim = '[' + fo.fname.substring(12) + ' (Calendar)]';\n\t\t\tj.level = 'days';\n\t\t\tvar d = fo.fvalue.split('|');\n\t\n\t\t\tj.filter = {\n\t\t\t\tfrom: d[0],\n\t\t\t\tto: d[1]\n\t\t\t};\n\t\t}\n\t\t\t\n\t\tjaql.metadata.push(j);\n\t}\t\n\t\n\t//assemble the jaql\n\tfor(var i = 0; i < dimPart.length; i++) {\n\t\tjaql.metadata.push(dimPart[i]);\n\t}\n\t\n\tfor(var i = 0; i < aggPart.length; i++) {\n\t\tjaql.metadata.push(aggPart[i]);\n\t}\n\n\tfor(var i = 0; i < filterPart.length; i++) {\n\t\tjaql.metadata.push(filterPart[i]);\n\t}\n\n\tconsole.log(JSON.stringify(jaql));\n\treturn jaql;\n\t\n}","subscription":{"isDataChange":true,"type":"onUpdate","schedule":"* * * * * *","timezone":300,"tzName":"America/New_York","context":{"dashboardid":"5c80821a42092517208b9829"},"active":true,"executionPerDay":5,"reportType":{"pdf":{"includeTitle":true,"includeFilters":false,"in